// This theme's colors are based on the original Monokai:
//   #1e1f1c (tab well, borders)
//   #0e0e0e (editor background)
//   #191919 (selection)
//   #75715e (focus)
//   #ffffff (editor foreground)
{
  "name": "Nathan",
  "type": "dark",
  "colors": {
    "dropdown.background": "#191919",
    "dropdown.listBackground": "#1e1f1c",
    "settings.textInputBackground": "#32342d",
    "settings.numberInputBackground": "#32342d",
    "list.activeSelectionBackground": "#0e0e0e",
    "list.focusBackground": "#191919",
    "list.inactiveSelectionBackground": "#0e0e0e",
    "list.hoverBackground": "#0e0e0e",
    "list.dropBackground": "#191919",
    "list.highlightForeground": "#ffffff",
    "button.background": "#75715E",
    "editor.background": "#0e0e0e",
    "editor.foreground": "#ffffff",
    "selection.background": "#ccccc7",
    "editor.selectionHighlightBackground": "#575b6180",
    "editor.selectionBackground": "#878b9180",
    "editor.wordHighlightBackground": "#4a4a7680",
    "editor.wordHighlightStrongBackground": "#6a6a9680",
    "editor.lineHighlightBackground": "#191919",
    "editorLineNumber.activeForeground": "#c2c2bf",
    "editorCursor.foreground": "#f8f8f0",
    "editorWhitespace.foreground": "#464741",
    "editorIndentGuide.background": "#464741",
    "editorIndentGuide.activeBackground": "#767771",
    "editorGroupHeader.tabsBackground": "#191919",
    "editorGroup.dropBackground": "#19191980",
    "tab.inactiveBackground": "#191919",
    "tab.activeBackground": "#0e0e0e",
    "tab.border": "#0e0e0e",
    "tab.inactiveForeground": "#ccccc7", // needs to be bright so it's readable when another editor group is focused
    "widget.shadow": "#000000",
    "progressBar.background": "#75715E",
    "badge.background": "#fa6d01",
    "badge.foreground": "#1a1a1a",
    "activityBarBadge.background": "#ffffff",
    "activityBarBadge.foreground": "#000000",
    "activityBar.background": "#191919",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#777",
    "activityBar.dropBackground": "#191919",
    "editorLineNumber.foreground": "#90908a",
    "panelTitle.activeForeground": "#ffffff",
    "panelTitle.activeBorder": "#75715E",
    "panelTitle.inactiveForeground": "#75715E",
    "panel.border": "#191919",
    "titleBar.activeBackground": "#191919",
    "titleBar.inactiveBackground": "#191919",
    "statusBar.background": "#191919",
    "statusBar.noFolderBackground": "#191919",
    "statusBar.debuggingBackground": "#75715E",
    "sideBar.background": "#191919",
    "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#0e0e0e",
    "menu.background": "#1e1f1c",
    "menu.foreground": "#cccccc",
    "pickerGroup.foreground": "#75715E",
    "input.background": "#191919",
    "inputOption.activeBorder": "#75715E",
    "focusBorder": "#75715E",
    "editorWidget.background": "#1e1f1c",
    "debugToolBar.background": "#1e1f1c",
    "gitDecoration.modifiedResourceForeground": "#fa6d01",
    "gitDecoration.addedResourceForeground": "#76FF03",
    "gitDecoration.untrackedResourceForeground": "#76FF03",
    "gitDecoration.deletedResourceForeground": "#FF1744",
    "gitDecoration.ignoredResourceForeground": "#555",
    "diffEditor.insertedTextBackground": "#66852880", // middle of #002627 and #76FF03
    "diffEditor.removedTextBackground": "#90274A80", // middle of #002627 and #F50057
    "inputValidation.errorBackground": "#90274A", // middle of #002627 and #F50057
    "inputValidation.errorBorder": "#F50057",
    "inputValidation.warningBackground": "#848528", // middle of #002627 and #e2e22e
    "inputValidation.warningBorder": "#e2e22e",
    "inputValidation.infoBackground": "#546190", // middle of #002627 and #819aff
    "inputValidation.infoBorder": "#819aff",
    "editorHoverWidget.background": "#191919",
    "editorHoverWidget.border": "#75715E",
    "editorSuggestWidget.background": "#002627",
    "editorSuggestWidget.border": "#75715E",
    "editorGroup.border": "#34352f",
    "peekView.border": "#75715E",
    "peekViewEditor.background": "#002627",
    "peekViewResult.background": "#1e1f1c",
    "peekViewTitle.background": "#1e1f1c",
    "peekViewResult.selectionBackground": "#191919",
    "peekViewResult.matchHighlightBackground": "#75715E",
    "peekViewEditor.matchHighlightBackground": "#75715E",
    "terminal.ansiBlack": "#333333",
    "terminal.ansiRed": "#C4265E", // the bright color with ~75% transparent on the background
    "terminal.ansiGreen": "#86B42B",
    "terminal.ansiYellow": "#B3B42B",
    "terminal.ansiBlue": "#6A7EC8",
    "terminal.ansiMagenta": "#8C6BC8",
    "terminal.ansiCyan": "#56ADBC",
    "terminal.ansiWhite": "#e3e3dd",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightRed": "#F50057",
    "terminal.ansiBrightGreen": "#76FF03",
    "terminal.ansiBrightYellow": "#e2e22e", // hue shifted #76FF03
    "terminal.ansiBrightBlue": "#819aff", // hue shifted #E040FB
    "terminal.ansiBrightMagenta": "#E040FB",
    "terminal.ansiBrightCyan": "#00E5FF",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#FFC400"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E040FB"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#E040FB"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#E040FB"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "meta.brace.round",
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "scope": [
        "meta.object-binding-pattern-variable",
        "meta.brace.square",
        "punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Object Key",
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#F50057"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00E5FF"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.type, entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#76FF03"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#76FF03"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#76FF03"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#F50057"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#76FF03"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#00E5FF"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#00E5FF"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00E5FF"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#F50057",
        "fontStyle": "",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#E040FB",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#76FF03"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#EEFF41"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#E040FBA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#EEFF41"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#EEFF41"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "#00E5FF"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#76FF03"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#76FF03"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FD971F"
      }
    }
  ]
}
